@model FDNE.PE.WEB.ADMIN.Dtos.CategoryDto

@Html.HiddenFor(m => m.Id)
<fieldset class="form-group">
    @Html.LabelFor(m => m.DisciplineId, new { @class = "label" })
    @Html.DropDownListFor(m => m.DisciplineId, new List<SelectListItem>(), new { @class = "form-control select2-single select2-disciplines", placeholder = "Disciplina", required = "required" })
</fieldset>
<fieldset class="form-group">
    @Html.LabelFor(m => m.Name, new { @class = "label" })
    @Html.TextBoxFor(m => m.Name, new { @class = "form-control", placeholder = "Nombre", required = "required", autocomplete = "off" })
</fieldset>
<fieldset class="form-group">
    @Html.LabelFor(m => m.Acronym, new { @class = "label" })
    @Html.TextBoxFor(m => m.Acronym, new { @class = "form-control", placeholder = "Acronimo", required = "required", autocomplete = "off" })
</fieldset>